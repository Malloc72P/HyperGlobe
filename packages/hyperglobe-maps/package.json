{
  "name": "@hyperglobe/maps",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "exports": {
    ".": {
      "import": "./dist/index.ts",
      "types": "./dist/index.ts"
    }
  },
  "scripts": {
    "prebuild:map": "tsx ./scripts/prepare-build.js",
    "build:map": "tsx ./src/script.ts",
    "postbuild:map": "tsx ./scripts/copy-bundle.js",
    "test": "vitest run",
    "release": "pnpm run build:map && cd ./publish/.npm && npm publish --access public"
  },
  "devDependencies": {
    "@hyperglobe/eslint-config": "workspace:*",
    "@hyperglobe/interfaces": "workspace:*",
    "@hyperglobe/node-tools": "workspace:*",
    "@hyperglobe/tools": "workspace:*",
    "@hyperglobe/typescript-config": "workspace:*",
    "@types/geojson": "^7946.0.8",
    "@types/node": "^24.10.0",
    "tsx": "^4.20.6",
    "vitest": "^4.0.1"
  }
}
