{
  "name": "myapp",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "PORT=42000 next start",
    "predev": "pnpm check-env && pnpm db-push && prisma generate",
    "dev": "dotenv -e .env.local -- next dev --turbopack",
    "prebuild": "pnpm check-env && prisma generate && pnpm meta",
    "build": "pnpm check-env && next build",
    "check-env": "dotenv -e .env.local -- tsx ./env-checker.ts",
    "lint": "next lint --max-warnings 0",
    "generate": "dotenv -e .env.local -- prisma generate",
    "db-push": "dotenv -e .env.local -- prisma db push",
    "seed": "dotenv -e .env.local -- tsx ./prisma/seed.ts",
    "sync": "pnpm generate && pnpm db-push && pnpm seed",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@mantine/core": "^8.1.0",
    "@mantine/hooks": "^8.1.0",
    "@mantine/modals": "^8.1.1",
    "@mantine/notifications": "^8.1.0",
    "@prisma/client": "^6.8.2",
    "@tabler/icons-react": "^3.26.0",
    "@tanstack/react-query": "^5.79.0",
    "@types/file-saver": "^2.0.7",
    "argon2": "^0.43.0",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "next": "^15.1.0",
    "next-auth": "^4.24.11",
    "next-nprogress-bar": "^2.4.3",
    "postcss": "^8.4.49",
    "postcss-preset-mantine": "^1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "seed": "ts-node prisma/seed.ts",
    "swr": "^2.3.0",
    "zod": "^3.25.28",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@myapp/eslint-config": "workspace:*",
    "@myapp/typescript-config": "workspace:*",
    "@next/bundle-analyzer": "^15.3.3",
    "@tanstack/react-query-devtools": "^5.79.0",
    "@types/node": "^20",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "dotenv-cli": "latest",
    "prisma": "^6.8.2",
    "tsx": "^4.19.4",
    "typescript": "5.5.4"
  }
}
