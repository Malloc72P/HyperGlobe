{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "dependsOn": ["build", "^build"],
      "cache": true
    },
    "seed": {
      "dependsOn": ["^seed"],
      "cache": false
    },
    "sync": {
      "dependsOn": ["^sync"],
      "cache": false
    },
    "meta": {
      "dependsOn": ["^meta"],
      "cache": false
    }
  }
}
